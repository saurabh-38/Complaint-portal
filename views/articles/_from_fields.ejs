<div class="form-group">

        <label for="title">Room Number and Hostel Name</label>
    <input required type="text" name="title" id="title" class="form-control" value="<%= article.title %>" > 

</div>
<div class="from-group">

        <label for="description">Available time</label>
   <textarea required name="description" id="description" class="form-control"  > <%= article.description %></textarea>
</div>



<div class="from-group">
        <label for="markdown">Problem description</label>
        <textarea required name="markdown" id="markdown" class="form-control"  ><%= article.markdown %></textarea>
  
</div>

<a href="/" class="btn  btn-secondary mt-4">Cancel</a>
                
<button type="submit" class="btn btn-primary mt-4">Save</button>


